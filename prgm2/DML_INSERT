CREATE TABLE product (
    p_id INT PRIMARY KEY,
    pname VARCHAR(30),
    price INT DEFAULT 0
);
mysql> desc product;
+-------+-------------+------+-----+---------+-------+
| Field | Type        | Null | Key | Default | Extra |
+-------+-------------+------+-----+---------+-------+
| p_id  | int         | NO   | PRI | NULL    |       |
| pname | varchar(30) | YES  |     | NULL    |       |
| price | int         | YES  |     | 0       |       |
+-------+-------------+------+-----+---------+-------+
3 rows in set (0.00 sec)







INSERT INTO product VALUES
(1, 'notebook', 15),
(2, 'pensil', 5),
(3, 'lipstick', 200),
(4, 'libalm', 150),
(5, 'calculator', 156),
(6, 'eraser', 2),
(7, 'scale', 10);




mysql> select * from product;
